{
  "name": "@amho/vottom-sheet",
  "description": "A sleek, highly responsive, touch-friendly, full-featured Bottom Sheet component for Vue.js",
  "version": "0.13.2",
  "private": true,
  "main": "./dist/index.umd.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.umd.js"
    },
    "./style.css": "./dist/style.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "test:unit": "vitest",
    "build-only": "vite build",
    "build:tsc": "vue-tsc --noEmit",
    "type-check": "pnpm run build:tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore",
    "lint:fix": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "test": "vitest",
    "prepare": "husky install"
  },
  "dependencies": {
    "@vueuse/core": "^10.5.0",
    "hammerjs": "^2.0.8",
    "happy-dom": "12.9.1",
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.8.0",
    "@commitlint/config-conventional": "^17.7.0",
    "@rushstack/eslint-patch": "^1.4.0",
    "@tsconfig/node18": "^18.2.2",
    "@types/hammerjs": "^2.0.42",
    "@types/jsdom": "^21.1.1",
    "@types/node": "^20.8.3",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vitejs/plugin-vue": "^4.4.0",
    "@vue/eslint-config-airbnb": "^7.0.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.1",
    "@vue/tsconfig": "^0.4.0",
    "eslint": "^8.51.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-vue": "^9.17.0",
    "husky": "^8.0.3",
    "jsdom": "^22.1.0",
    "lint-staged": "^14.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.3",
    "sass": "^1.69.0",
    "typescript": "~5.2.2",
    "vite": "^4.4.9",
    "vite-plugin-dts": "^3.6.0",
    "vitest": "^0.33.0",
    "vue-eslint-parser": "^9.3.1",
    "vue-tsc": "^1.8.19"
  },
  "homepage": "https://github.com/amirhoseinsalimi/vottom-sheet",
  "author": {
    "name": "Amir Hosein Salimi",
    "email": "ahosein.salimi@gmail.com",
    "url": "https://github.com/amirhoseinsalimi"
  },
  "keywords": [
    "vue",
    "vuejs",
    "bottomsheet",
    "bottom-sheet",
    "bottomsheets",
    "action-sheet",
    "actionsheet"
  ],
  "bugs": {
    "url": "https://github.com/amirhoseinsalimi/vottom-sheet/issues"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/amirhoseinsalimi/vottom-sheet.git"
  }
}
